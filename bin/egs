#!/bin/bash

ready() {
      . $Bin/egs
}

eg0() { 
    demo projected | xy -t "weather2" -z '$humidity' -o $Tmp/weather2.pdf -m 1 #2
}
eg1() {
      demo projected : proj1c | 
      xy  -o $Tmp/auto.pdf -t "autompg mpg" -z '-mpg' -c red -m 1
}
eg2() {
      demo projected : proj2d |
      xy  -o $Tmp/nasa93demDefects.pdf -t "nasa93dem Defects" -z '-defects' -m 1 -c red  -p 1 #2
}
eg3() {
      demo projected : proj2d |
      xy  -o $Tmp/nasa93demEffort.pdf -t "nasa93dem Effort" -z '-effort' -m 1 -c red -p 1 #2
}
eg4() {
      demo projected : proj2d |
      xy  -o $Tmp/nasa93demMonths.pdf -t "nasa93dem Months" -z '-months'  -m 1 -c red -p 1 #2
}
eg5() {
      demo projected : proj2e |
      xy  -o $Tmp/iris.pdf -t "iris class" -z '=class' -m 1 #-c red -p 1.5 
}
eg6() {
      demo projected : proj2f |
      xy  -o $Tmp/housing.pdf -t "housing class" -z '+class' -m 1 # -m 0.75 -c red -p 2
}
eg7() {
      demo projected : proj2g |
      xy  -o $Tmp/poi-30.pdf -t "poi-3.0 bugs" -z '-bugs' -m 1 -c red -p 1
}
